.navbar-minimalize{
	display:block;
	width:38px;
	height:30px;
	position:absolute;
	top:-10px;
	right:0;
	z-index:2;
	background:url("../images/btn_pullin.png") no-repeat 0 0;
}
.navbar-minimalize-click{
	background:url("../images/btn_pullout.png") no-repeat 0 0;
}
