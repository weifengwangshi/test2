.nav>li.list1{
    margin-top: 15px;
}
.nav>li>a{
    display: block;
    width: 220px;
    line-height: 50px;
    font-family: 'microsoft yahei';
    font-size: 14px;
    color: #a7b1c2;
    padding: 0 20px 0 25px;
}
.nav>li>a i{
    margin-right: 10px;
}
.arrow{
    margin-top: 16px;
}
.navbar-default .nav>li>a:focus, .navbar-default .nav>li>a:hover{background: rgba(0,0,0,0.6)}
